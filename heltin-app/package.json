{
  "name": "heltin-app",
  "version": "1.0.0",
  "description": "Robust client registry for individuals receiving mental healthcare services.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhidapa/heltin.git"
  },
  "bugs": {
    "url": "https://github.com/bhidapa/heltin/issues"
  },
  "homepage": "https://github.com/bhidapa/heltin#readme",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "test": "jest",
    "watch:test": "jest --watch",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "graphql:fix-schema": "node scripts/graphql/fix-schema.js",
    "graphql:get-schema": "get-graphql-schema http://localhost:5000/graphql --json > ./schema.json && npm run graphql:fix-schema ./schema.json",
    "relay:compile": "relay-compiler",
    "relay:start": "npm run relay:compile -- --watch",
    "build:dll": "webpack --config webpack.dll.config.js --hide-modules --display-chunks --color --progress",
    "build": "npm run relay:compile && webpack -p --config webpack.prod.config.js --display-optimization-bailout --profile --progress",
    "start:hot": "webpack-dev-server --config webpack.dev.config.js --progress",
    "start": "npm run build:dll && npm run start:hot"
  },
  "dll": {
    "name": "vendors",
    "path": "node_modules/.dll"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
