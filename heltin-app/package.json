{
  "name": "heltin-app",
  "version": "1.0.0",
  "description": "Robust client registry for individuals receiving mental healthcare services.",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bhidapa/heltin.git"
  },
  "bugs": {
    "url": "https://github.com/bhidapa/heltin/issues"
  },
  "homepage": "https://github.com/bhidapa/heltin#readme",
  "license": "SEE LICENSE IN LICENSE.md",
  "scripts": {
    "test": "jest",
    "watch:test": "jest --watch",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "graphql:fix-schema": "node scripts/graphql/fix-schema.js",
    "graphql:get-schema": "get-graphql-schema http://localhost:5000/graphql --json > ./schema.json && npm run graphql:fix-schema ./schema.json",
    "relay:compile": "relay-compiler",
    "relay:start": "npm run relay:compile -- --watch",
    "build:dll": "webpack --config webpack.dll.config.js --hide-modules --display-chunks --color --progress",
    "build": "npm run relay:compile && webpack -p --config webpack.prod.config.js --display-optimization-bailout --profile",
    "start:hot": "webpack-dev-server --config webpack.dev.config.js --progress",
    "start": "npm run build:dll && npm run start:hot"
  },
  "dll": {
    "name": "vendors",
    "path": "node_modules/.dll",
    "exclude": [
      "typeface-open-sans",
      "typeface-roboto-mono"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@testing-library/react": "^9.1.4",
    "@testing-library/react-hooks": "^2.0.1",
    "@types/jest": "^24.0.18",
    "@types/lodash": "^4.14.138",
    "@types/node": "^12.7.5",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-helmet": "^5.0.10",
    "@types/react-relay": "^5.0.0",
    "@types/react-router-dom": "^4.3.5",
    "@types/relay-runtime": "^5.0.5",
    "@types/storejs": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^2.2.0",
    "@typescript-eslint/parser": "^2.2.0",
    "add-asset-html-webpack-plugin": "^3.1.3",
    "babel-loader": "^8.0.6",
    "babel-plugin-relay": "^5.0.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "css-loader": "^3.2.0",
    "eslint": "^6.4.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "file-loader": "^4.2.0",
    "fork-ts-checker-webpack-plugin": "^1.5.0",
    "get-graphql-schema": "^2.1.2",
    "graphql": "^14.5.5",
    "html-webpack-plugin": "^3.2.0",
    "interpolate-html-plugin": "^3.0.0",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "react-test-renderer": "^16.9.0",
    "relay-compiler": "^5.0.0",
    "relay-compiler-language-typescript": "^4.7.0",
    "relay-config": "^5.0.0",
    "style-loader": "^1.0.0",
    "svg-inline-loader": "^0.8.0",
    "terser-webpack-plugin": "^2.0.1",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.1.0",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3",
    "url-loader": "^2.1.0",
    "webpack": "^4.40.2",
    "webpack-cli": "^3.3.8",
    "webpack-dev-server": "^3.8.0",
    "webpack-manifest-plugin": "^2.0.4",
    "webpack-merge": "^4.2.2",
    "workbox-webpack-plugin": "^4.3.1"
  },
  "dependencies": {
    "@domonda/query-params": "^1.2.0",
    "@domonda/react-form": "^2.2.1",
    "@domonda/react-plumb": "^1.1.1",
    "@domonda/ui": "^4.1.2",
    "clsx": "^1.0.4",
    "fast-equals": "^2.0.0",
    "history": "^4.10.1",
    "lodash": "^4.17.15",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-helmet": "^6.0.0-beta",
    "react-intl": "^3.2.1",
    "react-relay": "^5.0.0",
    "react-router-dom": "^5.0.1",
    "relay-runtime": "^5.0.0",
    "reselect": "^4.0.0",
    "store": "^2.0.12",
    "typeface-open-sans": "0.0.75",
    "typeface-roboto-mono": "0.0.75",
    "whatwg-fetch": "^3.0.0"
  }
}
