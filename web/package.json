{
  "private": true,
  "name": "heltin-app",
  "engines": {
    "node": ">=16",
    "yarn": ">=1.22.0"
  },
  "packageManager": "yarn@3.2.1",
  "scripts": {
    "get-schema-enums": "yarn relay-enum-generator --schema ../cmd/graphql/schema.graphql --path ./src --name enums.graphql.ts && echo \"/* tslint:disable */\n/* eslint-disable */\n// @ts-nocheck\n\n$(cat src/enums.graphql.ts)\" > src/enums.graphql.ts",
    "relay": "relay-compiler",
    "relay:clean": "rimraf $(find . -type d -name __generated__)",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "type-check": "tsc --noEmit",
    "build": "yarn vite build",
    "start": "yarn vite"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^6.1.1",
    "@popperjs/core": "^2.11.5",
    "@tanstack/react-location": "^3.7.4",
    "@tippyjs/react": "^4.2.6",
    "date-fns": "^2.28.0",
    "detect-browser": "^5.3.0",
    "downshift": "^6.1.7",
    "focus-trap-react": "^9.0.0",
    "print-js": "^1.6.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-dropzone": "^14.2.1",
    "react-helmet-async": "^1.3.0",
    "react-hook-form": "^7.32.0",
    "react-intl": "^6.0.4",
    "react-popper": "^2.3.0",
    "react-relay": "^14.0.0",
    "react-toastify": "^9.0.4",
    "relay-runtime": "^14.0.0",
    "tippy.js": "^6.3.7"
  },
  "devDependencies": {
    "@babel/core": "^7.18.2",
    "@trivago/prettier-plugin-sort-imports": "^3.2.0",
    "@types/babel__core": "^7.1.19",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-relay": "^13.0.2",
    "@types/relay-runtime": "^13.0.3",
    "@typescript-eslint/eslint-plugin": "^5.27.1",
    "@typescript-eslint/parser": "^5.27.1",
    "@vitejs/plugin-react": "^1.3.2",
    "babel-plugin-relay": "^14.0.0",
    "browserslist": "^4.20.4",
    "eslint": "^8.17.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.30.0",
    "prettier": "^2.6.2",
    "relay-compiler": "^14.0.0",
    "relay-config": "^12.0.1",
    "relay-enum-generator": "^0.0.4",
    "rimraf": "^3.0.2",
    "typescript": "^4.7.3",
    "vite": "^2.9.11",
    "vite-tsconfig-paths": "^3.5.0"
  }
}
